{
  "name": "plugin-datepicker",
  "version": "0.2.29",
  "description": "A versatile and customizable React DatePicker component with an integrated calendar for easy date selection.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "keywords": [
    "datepicker",
    "date-picker",
    "calendar",
    "react-datepicker",
    "input",
    "form",
    "date-selection"
  ],
  "author": {
    "name": "vangit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VangitParis/plugin-datepicker.git"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "eslintrc": "^1.0.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
